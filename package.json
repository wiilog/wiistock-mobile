{
    "name": "wiistock-mobile",
    "version": "7.14.0",
    "author": "Wiilog Team",
    "description": "Follow your supply chain with GT Logistics.",
    "homepage": "https://www.wiilog.fr/",
    "license": "proprietary",
    "scripts": {
        "ng": "ng",
        "start": "yarn ng serve",
        "lint": "yarn ng lint",
        "sync": "yarn cordova-res android --skip-config --copy && yarn capacitor sync",

        "deprecated:build": "yarn ng build --configuration production && yarn sync",
        "deprecated:build:dev": "yarn ng build --configuration development && yarn sync",
        "deprecated:build:android": "yarn deprecated:build && yarn deprecated:android:open",
        "deprecated:android:open": "yarn capacitor copy && yarn capacitor copy android && yarn capacitor open android",
        "deprecated:watch": "yarn deprecated:build:dev && yarn ionic capacitor run android --livereload --external",

        "capacitor:copy": "yarn capacitor copy && yarn capacitor copy android",
        "angular:build": "yarn ng build --configuration production && yarn sync",
        "angular:build:dev": "yarn ng build --configuration development && yarn sync",
        "gradle:assemble": "yarn angular:build && yarn capacitor:copy && yarn gradle clean && yarn gradle assembleRelease",
        "gradle:assemble:debug": "yarn angular:build:dev && yarn capacitor:copy && yarn gradle clean && yarn gradle assembleDebug",
        "gradle": "./android/gradlew --project-dir ./android",
        "watch": "yarn angular:build:dev && yarn ionic capacitor run android --livereload --external --public-host=$HOST_IP",

        "wiis:version": "node scripts/set-app-version"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^15.0.0",
        "@angular/core": "^15.0.0",
        "@angular/forms": "^15.0.0",
        "@angular/platform-browser": "^15.0.0",
        "@angular/platform-browser-dynamic": "^15.0.0",
        "@angular/router": "^15.0.0",
        "@capacitor-community/date-picker": "^5.0.0",
        "@capacitor-community/fcm": "^5.0.1",
        "@capacitor-community/sqlite": "^5.0.6",
        "@capacitor/android": "^5.0.0",
        "@capacitor/app": "^5.0.0",
        "@capacitor/browser": "^5.0.0",
        "@capacitor/camera": "^5.0.0",
        "@capacitor/core": "^5.0.0",
        "@capacitor/haptics": "^5.0.0",
        "@capacitor/keyboard": "^5.0.0",
        "@capacitor/local-notifications": "^5.0.0",
        "@capacitor/network": "^5.0.0",
        "@capacitor/preferences": "^5.0.0",
        "@capacitor/push-notifications": "^5.0.0",
        "@capacitor/splash-screen": "^5.0.0",
        "@capacitor/status-bar": "^5.0.0",
        "@ionic/angular": "^6.1.9",
        "@ionic/cli": "^6.20.8",
        "@types/bcryptjs": "^2.4.2",
        "@types/leaflet": "^1.9.0",
        "bcryptjs": "^2.4.3",
        "com-darryncampbell-cordova-plugin-intent": "^2.2.0",
        "cordova-res": "^0.15.4",
        "crypto-browserify": "^3.12.0",
        "ionic-selectable": "^4.9.0",
        "ionicons": "^6.0.3",
        "leaflet": "^1.9.3",
        "moment": "^2.29.4",
        "open-sans-fonts": "^1.6.2",
        "phonegap-plugin-barcodescanner": "github:wiilog/phonegap-plugin-barcodescanner",
        "rxjs": "~7.5.0",
        "stream-browserify": "^3.0.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.0.0",
        "@angular-eslint/builder": "^14.0.0",
        "@angular-eslint/eslint-plugin": "^14.0.0",
        "@angular-eslint/eslint-plugin-template": "^14.0.0",
        "@angular-eslint/template-parser": "^14.0.0",
        "@angular/cli": "^15.0.0",
        "@angular/compiler": "^15.0.0",
        "@angular/compiler-cli": "^15.0.0",
        "@angular/language-service": "^15.0.0",
        "@capacitor/cli": "^5.0.0",
        "@ionic/angular-toolkit": "^6.0.0",
        "@types/jasmine": "~4.0.0",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.3.0",
        "@typescript-eslint/parser": "5.3.0",
        "eslint": "^7.6.0",
        "eslint-plugin-import": "2.22.1",
        "eslint-plugin-jsdoc": "30.7.6",
        "eslint-plugin-prefer-arrow": "1.2.2",
        "jasmine-core": "~4.3.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "ts-node": "~8.3.0",
        "typescript": "~4.8.4"
    }
}
